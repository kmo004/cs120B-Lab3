 #include <avr/io.h>
 #ifdef _SIMULATE_
 #include "simAVRHeader.h"
 #endif

int main(void){
	DDRA = 0x00; PORTA = 0xFF;
	DDRC = 0xFF; PORTC = 0x00;
  ignitionMask = 0x10;
  seatedMask = 0x20;
  sbYesMask = 0x40;
 
  
	
	if((PINA & 0x00) == 0x00){
		PORTC = 0x40;
	}
	if((PINA & 0x00) == 0x01){
		PORTC = 0x60;
	}
	if((PINA & 0x00) == 0x02){
		PORTC = 0x60;
	}
	if((PINA & 0x00) == 0x03){
		PORTC = 0x70;
	}
	if((PINA & 0x00) == 0x04){
		PORTC = 0x70;
	}
	if((PINA & 0x00) == 0x05){
		PORTC = 0x38;
	}
	if((PINA & 0x00) == 0x06){
		PORTC = 0x38;
	}
	if((PINA & 0x00) == 0x07){
		PORTC = 0x3C;
	}
	if((PINA & 0x00) == 0x08){
		PORTC = 0x3C;
	}
	if((PINA & 0x00) == 0x09){
		PORTC = 0x3E;
	}
	if((PINA & 0x00) == 0x0A){
		PORTC = 0x3C;
	}
	if((PINA & 0x00) == 0x0B){
		PORTC = 0x3E;
	}
	if((PINA & 0x00) == 0x0C){
		PORTC = 0x3E;
	}
	if((PINA & 0x00) == 0x0D){
		PORTC = 0x3F;
	}
	if((PINA & 0x00) == 0x0E){
		PORTC = 0x3F;
	}
	if((PINA & 0x00) == 0x0F){
		PORTC = 0x3F;
	}
  if((PINA & ignitionMask) == 0x10){
    if(PINA & seatedMask) == 0x20){
      if(PINA & sbYesMask) != 0x40){
        PORTC = PORTC + 0xA0
      }
    }
  }
    

		
	
	
	return 1;
}
